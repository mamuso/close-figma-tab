{
  "name": "Close Zoom Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Closes the Zoom launch session browser tab.",
  "homepage_url": "https://github.com/ericwbailey/close-zoom-tab/",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
    "matches": ["http://*.figma.com/file/*", "https://*.figma.com/file/*"],
     "js": ["js/content.js"],
     "run_at": "document_end"
    }
   ], 
  "permissions": [
    "activeTab"
  ]
}